{
  "name": "dialute",
  "version": "0.0.25",
  "author": "Din Dmitriy <@Dikower>",
  "description": "A highly declarative framework for building SberSalute apps",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dikower/Dialute.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.ts\"",
    "lint": "eslint .",
    "dev": "ts-nodemon --no-notify --entry dist/tests.js",
    "patch": "pnpm version patch --no-git-tag-version"
  },
  "dependencies": {
    "body-parser": "^1.20.0",
    "chalk": "^2.4.2",
    "express": "^4.18.0"
  },
  "devDependencies": {
    "@trongnd/ts-nodemon": "^1.0.3",
    "@types/express": "^4.17.13",
    "@types/node": "^15.14.9",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.6.2",
    "ts-node": "^10.7.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.6.3"
  }
}
