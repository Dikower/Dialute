{
  "name": "dialute",
  "version": "0.0.23",
  "author": "Din Dmitriy <@Dikower>",
  "description": "A highly declarative framework for building SberSalute apps",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dikower/Dialute.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "dev": "ts-nodemon --no-notify --entry dist/tests.js",
    "publish": "pnpm run build && pnpm version patch "
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "chalk": "^2.4.2",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@trongnd/ts-nodemon": "^1.0.3",
    "@types/express": "^4.17.12",
    "@types/node": "^15.12.1",
    "prettier": "^2.3.1",
    "ts-node": "^10.7.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.3.2"
  }
}
